@model IEnumerable<EstablishmentModel>

@{
    ViewBag.Title = "Index";
}

<div>
    <table>
        <tr>
            <th>@Html.LabelFor(model => model.First().Id)</th>
            <th>@Html.LabelFor(model => model.First().Name)</th>
            <th>@Html.LabelFor(model => model.First().Address)</th>
            <th>@Html.LabelFor(model => model.First().PostCode)</th>
            <th>@Html.LabelFor(model => model.First().Town)</th>
            <th>@Html.LabelFor(model => model.First().User_Id)</th>
            <th>@Html.LabelFor(model => model.First().Academie_Id)</th>
            <th>@Html.LabelFor(model => model.First().Academies)</th>
            <th>@Html.LabelFor(model => model.First().Classrooms)</th>
            <th>@Html.LabelFor(model => model.First().Users)</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Address)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PostCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Town)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.User_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Academie_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Academies)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Classrooms)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Users)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { onclick = "return confirm('Are you sure you wish to delete this Academy?');" })
                </td>
            </tr>
        }
    </table>
    @Html.ActionLink("Create new User", "Create")
</div>
