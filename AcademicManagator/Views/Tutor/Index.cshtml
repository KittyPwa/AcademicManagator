@model IEnumerable<TutorModel>

@{
    ViewBag.Title = "Index";
}

<div>
    <table>
        <tr>
            <th>@Html.LabelFor(model => model.First().Id)</th>
            <th>@Html.LabelFor(model => model.First().LastName)</th>
            <th>@Html.LabelFor(model => model.First().FirstName)</th>
            <th>@Html.LabelFor(model => model.First().Address)</th>
            <th>@Html.LabelFor(model => model.First().PostCode)</th>
            <th>@Html.LabelFor(model => model.First().Town)</th>
            <th>@Html.LabelFor(model => model.First().Tel)</th>
            <th>@Html.LabelFor(model => model.First().Mail)</th>
            <th>@Html.LabelFor(model => model.First().Comment)</th>
            <th>@Html.LabelFor(model => model.First().Pupils)</th>




            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Address)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PostCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Town)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Tel)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Mail)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Comment)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pupils)
                </td>
            
                <td>
                    @Html.ActionLink("Edit", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { onclick = "return confirm('Are you sure you wish to delete this Academy?');" })
                </td>
            </tr>
            }
        </table>
        @Html.ActionLink("Creer nouvel tuteur", "Create")
    </div>
