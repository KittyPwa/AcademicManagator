@model IEnumerable<EvaluationModel>

@{
    ViewBag.Title = "Index";
}

<div>
    <table>
        <tr>
            <th>@Html.LabelFor(model => model.First().Id)</th>
            <th>@Html.LabelFor(model => model.First().Classroom_Id)</th>
            <th>@Html.LabelFor(model => model.First().User_Id)</th>
            <th>@Html.LabelFor(model => model.First().Period_Id)</th>
            <th>@Html.LabelFor(model => model.First().Date)</th>
            <th>@Html.LabelFor(model => model.First().TotalPoint)</th>
            <th>@Html.LabelFor(model => model.First().Classrooms)</th>
            <th>@Html.LabelFor(model => model.First().Periods)</th>
            <th>@Html.LabelFor(model => model.First().Users)</th>
            <th>@Html.LabelFor(model => model.First().Results)</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Classroom_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.User_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Period_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TotalPoint)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Classrooms)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Periods)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Classrooms)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Users)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Results)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { onclick = "return confirm('Are you sure you wish to delete this Academy?');" })
                </td>
            </tr>
        }
    </table>
    @Html.ActionLink("Creer nouvelle feuille d'evaluation", "Create")
</div>
